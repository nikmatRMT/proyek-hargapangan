{
  "version": 2,
  "builds": [
    {
      "src": "harga-backend/src/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "web-admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "harga-backend/src/server.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "harga-backend/src/server.js"
    },
    {
      "src": "/m/(.*)",
      "dest": "harga-backend/src/server.js"
    },
    {
      "src": "/sse/(.*)",
      "dest": "harga-backend/src/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "harga-backend/src/server.js"
    },
    {
      "src": "/health",
      "dest": "harga-backend/src/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|txt))",
      "dest": "/web-admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web-admin/index.html"
    }
  ]
}
